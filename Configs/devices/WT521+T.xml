<DeviceEmulator DeviceName="WT521 (W/T/H, rand)" IsTimeIncluded="False">
  <Message TimeInterval="1000" FormatString="$WIMWV,{0},R,{1},M,A*3A&#xD;&#xA;">
    <ValueSet Name="Wind Direction" RandomValuesEnabled="True" ValueFormat="F0" LowerBound="50" UpperBound="150" />
	<ValueSet Name="Wind Speed" RandomValuesEnabled="True" ValueFormat="F1" LowerBound="2" UpperBound="7" />
  </Message>
  <Message TimeInterval="15000" FormatString="$WIXDR,C,{0},C,0,H,{1},P,0*3A&#xD;&#xA;">
    <ValueSet Name="Temperature" RandomValuesEnabled="True" ValueFormat="F1" LowerBound="3" UpperBound="6" />
	<ValueSet Name="Humidity" RandomValuesEnabled="True" ValueFormat="F0" LowerBound="60" UpperBound="70" />
  </Message>
</DeviceEmulator>